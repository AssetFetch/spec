{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"title": "Datablock: format.blend",
	"$id": "af.datablock.format.blend",
	"description": "Information about files with the extension `.blend`.",
	"type": "object",
	"properties": {
		"version": {
			"type": [
				"string",
				"null"
			],
			"pattern": "^[0-9]+(\\.[0-9]+)?(\\.[0-9]+)?$"
		},
		"is_asset": {
			"type": [
				"boolean",
				"null"
			]
		},
		"targets": {
			"type": [
				"array",
				"null"
			],
			"items": {
				"type": "object",
				"required": [
					"kind",
					"names"
				],
				"properties": {
					"kind": {
						"type": "string",
						"enum": [
							"actions",
							"armatures",
							"brushes",
							"cache_files",
							"cameras",
							"collections",
							"curves",
							"fonts",
							"grease_pencils",
							"hair_curves",
							"images",
							"lattices",
							"lightprobes",
							"lights",
							"linestyles",
							"masks",
							"materials",
							"meshes",
							"metaballs",
							"movieclips",
							"node_groups",
							"objects",
							"paint_curves",
							"palettes",
							"particles",
							"pointclouds",
							"scenes",
							"screens",
							"simulations",
							"sounds",
							"speakers",
							"texts",
							"textures",
							"volumes",
							"workspaces",
							"worlds"
						]
					},
					"names": {
						"type": "array",
						"items": {
							"type": "string"
						}
					}
				}
			}
		}
	},
	"required": []
}